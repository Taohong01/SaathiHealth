     "collapsed": false,
     "input": [
      "import numpy as np\n",
      "import matplotlib.pyplot as plt\n",
      "from sklearn import metrics\n",
      "\n",
      "from sklearn.linear_model import LogisticRegression\n",
      "from sklearn import datasets\n",
      "from sklearn.preprocessing import StandardScaler\n",
      "\n",
      "import pandas as pds\n",
      "import pandas.io.excel as pdexl\n",
      "\n",
      "datafile2 = 'c://DIP02.csv'\n",
      "\n",
      "df2 = pds.read_csv(datafile2, skiprows = 3, sep=',').T\n",
      "\n",
      "#print df2\n",
      "\n",
      "numSamples = 10\n",
      "df4 = df2.T\n",
      "dfBounded = df4.iloc[0:230, 0:53]\n",
      "#print dfBounded\n",
      "dfNoEmptyRows = dfBounded.dropna(axis = 0, how='any')\n",
      "#print dfNoEmptyRows.iloc[0:,0:53]\n",
      "dfHTML=dfNoEmptyRows.T.to_html(open('DataOutPut2.html', 'w'))\n",
      "#print dfHTML\n",
      "dfFea_Samp = dfNoEmptyRows.T\n",
      "#print dfFea_Samp.ix[0]\n",
      "#print dfFea_Samp.ix[0:23]\n",
      "dfFea_Samp_RenameCol = pds.DataFrame(dfFea_Samp.ix[2:53].values, columns=dfFea_Samp.ix[0], dtype=np.float32)\n",
      "#print dfFea_Samp_RenameCol[['Tobacco','y']]\n",
      "#print dfFea_Samp_RenameCol.columns\n",
      "Y = dfFea_Samp_RenameCol[['Coronary Dz/Event? ','Active symptoms?']]\n",
      "X = dfFea_Samp_RenameCol.drop(['Coronary Dz/Event? ','Active symptoms?','Height (in)'], axis=1) #\n",
      "Xcolname = list(X.columns)\n",
      "#print Xcolname\n",
      "\n",
      "\n",
      "#print X\n",
      "#print Y\n",
      "X = X.values\n",
      "y = Y[ ['Active symptoms?'] ].values\n",
      "#print X\n",
      "Xmax = X.max(axis=0)\n",
      "Xmin = X.min(axis=0)\n",
      "Xnormal = (X-Xmin)/(Xmax-Xmin)\n",
      "X = Xnormal\n",
      "y=np.ravel(y)\n",
      "#print y\n",
      "\n",
      "\n",
      "# Set regularization parameter\n",
      "for i, C in enumerate((100,10, 1, 0.01)):\n",
      "    print 'i=',i\n",
      "    print 'C=',C\n",
      "    # turn down tolerance for short training time\n",
      "    clf_l1_LR = LogisticRegression(C=C, penalty='l1', tol=0.01)\n",
      "    clf_l2_LR = LogisticRegression(C=C, penalty='l2', tol=0.01)\n",
      "    clf_l1_LR.fit(X, y)\n",
      "    clf_l2_LR.fit(X, y)\n",
      "    print 'training is done'\n",
      "\n",
      "    coef_l1_LR = clf_l1_LR.coef_.ravel()\n",
      "    coef_l2_LR = clf_l2_LR.coef_.ravel()\n",
      "    print 'Fitting Coef in case of L1:', pds.DataFrame(coef_l1_LR , index=Xcolname)\n",
      "    #print 'fitting coef L2:', coef_l2_LR\n",
      "\n",
      "    # coef_l1_LR contains zeros due to the\n",
      "    # L1 sparsity inducing norm\n",
      "\n",
      "    sparsity_l1_LR = np.mean(coef_l1_LR == 0) * 100\n",
      "    sparsity_l2_LR = np.mean(coef_l2_LR == 0) * 100\n",
      "    \n",
      "    print\n",
      "    print 'Model Evluation:'\n",
      "    print(\"C=%.2f\" % C)\n",
      "    print(\"Sparsity with L1 penalty: %.2f%%\" % sparsity_l1_LR)\n",
      "    print(\"score with L1 penalty: %.4f\" % clf_l1_LR.score(X, y))\n",
      "    print(\"Sparsity with L2 penalty: %.2f%%\" % sparsity_l2_LR)\n",
      "    print(\"score with L2 penalty: %.4f\" % clf_l2_LR.score(X, y))\n",
      "    print '------------------------------------------------------------'\n",
      "    print\n",
      "    \n",
      "    \n",
      "y_true = y.ravel()\n",
      "print 'Active symptoms?', y_true\n",
      "\n",
      "XX = dfFea_Samp_RenameCol.drop(['Coronary Dz/Event? ','Active symptoms?','Height (in)'], axis=1)\n",
      "y_pred = XX['QRISK2 Score'].values\n",
      "print 'score value', y_pred\n",
      "y_pred = y_pred.ravel() >1.4\n",
      "print 'prediction based on score value is', y_pred\n",
      "print '---------Evaluation of QRISK2 Score ------'\n",
      "print 'Precision is', metrics.precision_score(y_true, y_pred)\n",
      "print 'Recall  is', metrics.recall_score(y_true, y_pred)\n",
      "print 'Accuracy is', metrics.accuracy_score(y_true, y_pred, normalize=False)\n",
      "print 'f1 score is', metrics.f1_score(y_true, y_pred)\n",
      "print '------------------------------------------'\n",
      "print\n",
      "print\n",
      "\n",
      "y_pred = XX['QRISK typical'].values\n",
      "print 'score value', y_pred\n",
      "y_pred = y_pred.ravel() >1.4\n",
      "print 'prediction based on score value is', y_pred\n",
      "print '---------Evaluation of QRISK typical ------'\n",
      "print 'Precision is', metrics.precision_score(y_true, y_pred)\n",
      "print 'Recall  is', metrics.recall_score(y_true, y_pred)\n",
      "print 'Accuracy is', metrics.accuracy_score(y_true, y_pred, normalize=False)\n",
      "print 'f1 score is', metrics.f1_score(y_true, y_pred)\n",
      "print '------------------------------------------'\n",
      "print\n",
      "print\n",
      "\n",
      "\n",
      "\n",
      "\n",
      "\n"
     ],
